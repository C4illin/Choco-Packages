<?xml version="1.0" encoding="utf-8"?>
<!-- Do not remove this test for UTF-8: if “Ω” doesn’t appear as greek uppercase omega letter enclosed in quotation marks, you should use an editor that supports UTF-8, not this one. -->
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <tags></tags>
    <authors></authors>
    <copyright></copyright> <!-- Years + author -->
    <iconUrl></iconUrl> <!-- Use a CDN ex. https://githack.com -->
    <projectUrl></projectUrl> <!-- Main url, main page -->
    <licenseUrl></licenseUrl> <!-- link to their license -->
    <projectSourceUrl></projectSourceUrl> <!-- Project source code -->
    <docsUrl></docsUrl>
    <bugTrackerUrl></bugTrackerUrl>
    <releaseNotes></releaseNotes> <!-- Probably a URL -->
    <description></description>
    <summary></summary>

    <id><%=package_name%></id>
    <version></version>    
    <title><%=title%></title> <!-- Good looking title -->
    <packageSourceUrl>https://github.com/C4illin/Choco-Packages/tree/master/<%=package_name%></packageSourceUrl>
    <!-- Dont change below here -->
    <owners>C4illin</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
